{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "debug": "nodemon ./src --inspect=0.0.0.0:9222  -r ts-node/register ./src/index.ts",
    "debug:watch": "nodemon --watch ./src --inspect=0.0.0.0:9222  -r ts-node/register ./src/index.ts",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@types/exif": "^0.6.0",
    "@types/express": "^4.16.1",
    "@types/node": "^6.0.50",
    "@types/node-cache": "^4.1.3",
    "@types/recursive-readdir": "^2.2.0",
    "@types/rx": "^4.1.1",
    "@types/sharp": "^0.22.2",
    "@types/walk": "^2.3.0",
    "console-stamp": "^0.2.7",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "exif": "^0.6.0",
    "express": "^4.16.4",
    "image-type": "^4.1.0",
    "is-image": "^3.0.0",
    "node-cache": "^4.2.0",
    "node-ssh": "^6.0.0",
    "node-ts-cache": "^2.0.0",
    "read-chunk": "^3.2.0",
    "recursive-readdir": "^2.2.2",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "sharp": "^0.22.1",
    "ts-node": "^8.1.0",
    "typescript": "^2.3.2",
    "walk": "^2.3.14"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
